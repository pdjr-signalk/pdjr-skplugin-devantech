  "dependencies": {
    "net": "^1.0.2",
    "signalk-libdelta": "github:pdjr-signalk/signalk-libdelta",
    "sprintf-js": "^1.1.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
